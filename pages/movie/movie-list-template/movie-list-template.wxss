
.movie-box .middle{
  display: flex;
  flex-wrap: wrap; /* 让子元素多行显示 */
}
.movie-box .middle .list{
  width: 33%;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 10px 0;
}
.movie-box .middle .list image{
   width: 100px;
   height: 140px;
}
.movie-box .middle .list .title{
   font-weight: bold;
   padding: 4px 0;
}

@import '../movie-star-template/movie-star-template.wxss'
